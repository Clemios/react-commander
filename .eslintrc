{

    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "import/extensions": 0,
        "jsx-a11y/anchor-is-valid": [
            "error",
            {
                "components": [
                    "Link"
                ],
                "specialLink": [
                    "to"
                ]
            }
        ]
    },
    "globals": {
      "document": true
    },
    "plugins": [
        "transform-class-properties"
    ],
    "parser": "babel-eslint"
}